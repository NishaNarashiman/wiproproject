using System;

public class HelloWorld 
{
    public static void Main(string[] args) 
    {
        int num = 123;
        int reversed = 0;
        while (num > 0) 
        {
            reversed = reversed * 10 + num % 10;
            num /= 10;
        }
        Console.WriteLine("Reversed: " + reversed);
        
    }
}