using System;

class Program
{
    static void Main()
    {
        int n = 10;
        Console.WriteLine($"Fibonacci number at position {n} is {Fibonacci(n)}");
    }

    static long Fibonacci(int n)
    {
        if (n <= 1)
            return n;
        else
            return Fibonacci(n - 1) + Fibonacci(n - 2);
    }
}
