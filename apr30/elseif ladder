using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int marks=78;
        if (marks>=90)
        {
            Console.WriteLine("grade: A");
        }
        else if (marks>=75)
        {
            Console.WriteLine("grade: B");
        }
        else if (marks>=60)
        {
            Console.WriteLine("grade: C");
        }
        else
        {
            Console.WriteLine("grade: D");
        }
    }
}